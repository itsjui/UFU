void somaimpar(Lista *l)
{   
    int soma = 0;
    int i = l->cabeca->info;

    if(vazia(l) = 1)
    return 0;
    
    if(i % 2 != 0){
    
        soma += i;
        l->cabeca=l->cabeca->prox;
        return i+somaimpar(l);
    }
    else{
        l->cabeca=l->cabeca->prox;
        return somaimpar(l);
    }
    
}